  other-modules:
      Capnp.Gen.Echo
    , Capnp.Gen.Echo.Pure
    , Capnp.Gen.ById.Xd0a87f36fa0182f5
    , Capnp.Gen.ById.Xd0a87f36fa0182f5.Pure
  build-depends:
      base ^>=4.11
    , capnp
    , bytestring ^>=0.10
    , data-default ^>=0.7
    , exceptions ^>=0.10
    , vector ^>=0.12
    , network ^>= 2.8.0
    , network-simple ^>= 0.4
  -- hs-source-dirs:
  default-language:    Haskell2010
